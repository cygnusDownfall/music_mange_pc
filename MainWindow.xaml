<Window x:Class="music_manage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:music_manage"
        mc:Ignorable="d"
        Title="MainWindow" Height="1000" Width="800">
    <Grid>
        <StackPanel Width="800" Height="auto" Background="Black"  HorizontalAlignment="Center">
            <Grid Height="50" Background="GreenYellow">
                
            </Grid>
            <StackPanel Width="700" Height="500">
                <uc:travelmainboard>
                    
                </uc:travelmainboard>
                    <Slider Background="Yellow" BorderBrush="Aquamarine" Width="600" Value="50" Maximum="100" Margin="20,10,10,10">

                    </Slider>
                
                <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" Margin="10,10,10,10">
                    <Button Width="100" Height="50"/>
                    <Button Width="100" Height="50"/>
                    <Button Width="100" Height="50"/>
                    <Button Width="100" Height="50"/>
                    <Button Width="100" Height="50"/>
                </StackPanel>
            </StackPanel>
            

        </StackPanel>
    </Grid>
</Window>
